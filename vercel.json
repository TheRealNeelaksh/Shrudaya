{
    "version": 2,
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "maxLambdaSize": "100mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/web/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ]
}